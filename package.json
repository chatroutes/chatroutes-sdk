{
  "name": "chatroutes-sdk",
  "version": "0.2.0",
  "description": "Official TypeScript/JavaScript SDK for ChatRoutes - Conversation Branching Platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "echo \"No tests yet\" && exit 0",
    "test:watch": "vitest",
    "test:coverage": "echo \"No tests yet\" && exit 0",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "chatroutes",
    "ai",
    "conversation",
    "branching",
    "llm",
    "gpt",
    "claude",
    "chat",
    "sdk",
    "typescript"
  ],
  "author": {
    "name": "Afzal Farooqui",
    "email": "support@chatroutes.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chatroutes/chatroutes-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/chatroutes/chatroutes-sdk/issues"
  },
  "homepage": "https://github.com/chatroutes/chatroutes-sdk#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "eventsource-parser": "^1.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
